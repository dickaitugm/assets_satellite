  <!-- ===== GEOMETRY ===== -->
  <library_geometries>
    <geometry id="AxisBox-Geom" name="AxisBox">
      <mesh>
        <!-- Vertex positions -->
        <source id="positions">
          <float_array id="positions-array" count="24">
            -0.5 -0.5 -0.5
            -0.5  0.5 -0.5
             0.5  0.5 -0.5
             0.5 -0.5 -0.5
            -0.5 -0.5  0.5
            -0.5  0.5  0.5
             0.5  0.5  0.5
             0.5 -0.5  0.5
          </float_array>
          <technique_common>
            <accessor source="#positions-array" count="8" stride="3">
              <param name="X" type="float"/>
              <param name="Y" type="float"/>
              <param name="Z" type="float"/>
            </accessor>
          </technique_common>
        </source>

        <!-- Normals per face -->
        <source id="normals">
          <float_array id="normals-array" count="18">
             1 0 0    <!-- +X -->
            -1 0 0    <!-- -X -->
             0 1 0    <!-- +Y -->
             0 -1 0   <!-- -Y -->
             0 0 1    <!-- +Z -->
             0 0 -1   <!-- -Z -->
          </float_array>
          <technique_common>
            <accessor source="#normals-array" count="6" stride="3">
              <param name="X" type="float"/>
              <param name="Y" type="float"/>
              <param name="Z" type="float"/>
            </accessor>
          </technique_common>
        </source>

        <!-- Vertices -->
        <vertices id="verts">
          <input semantic="POSITION" source="#positions"/>
        </vertices>

        <!-- Faces by material -->
        <triangles material="Mat-Xpos" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>3 0 2 0 6 0 3 0 6 0 7 0</p>
        </triangles>

        <triangles material="Mat-Xneg" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>0 1 1 1 5 1 0 1 5 1 4 1</p>
        </triangles>

        <triangles material="Mat-Ypos" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>1 2 2 2 6 2 1 2 6 2 5 2</p>
        </triangles>

        <triangles material="Mat-Yneg" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>0 3 3 3 7 3 0 3 7 3 4 3</p>
        </triangles>

        <triangles material="Mat-Zpos" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>4 4 5 4 6 4 4 4 6 4 7 4</p>
        </triangles>

        <triangles material="Mat-Zneg" count="2">
          <input semantic="VERTEX" source="#verts" offset="0"/>
          <input semantic="NORMAL" source="#normals" offset="1"/>
          <p>0 5 1 5 2 5 0 5 2 5 3 5</p>
        </triangles>
      </mesh>
    </geometry>
  </library_geometries>

  <!-- ===== SCENE ===== -->
  <library_visual_scenes>
    <visual_scene id="Scene">
      <node id="AxisBoxNode" name="AxisBox">
        <instance_geometry url="#AxisBox-Geom">
          <bind_material>
            <technique_common>
              <instance_material symbol="Mat-Xpos" target="#Mat-Xpos"/>
              <instance_material symbol="Mat-Xneg" target="#Mat-Xneg"/>
              <instance_material symbol="Mat-Ypos" target="#Mat-Ypos"/>
              <instance_material symbol="Mat-Yneg" target="#Mat-Yneg"/>
              <instance_material symbol="Mat-Zpos" target="#Mat-Zpos"/>
              <instance_material symbol="Mat-Zneg" target="#Mat-Zneg"/>
            </technique_common>
          </bind_material>
        </instance_geometry>
      </node>
    </visual_scene>
  </library_visual_scenes>

  <scene>
    <instance_visual_scene url="#Scene"/>
  </scene>
</COLLADA>
